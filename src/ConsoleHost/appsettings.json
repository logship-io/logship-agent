{
  "Output": {
    "endpoint": "http://localhost:5000",
    "subscription": "00000000-0000-0000-0000-000000000000",
    "interval": "00:00:02",
    "maximumBufferSize": 40000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "Inputs": [
    {
      "type": "HealthService",
      "interval": "00:00:05.000"
    },
    {
      "type": "Windows.PerformanceCounters",
      "interval": "00:00:05",
      "counters": [
        "\\Process(*)\\*logship*"
      ]
    },
    {
      "type": "Windows.Etw",
      "cleanupOldSessions": true,
      "reuseExistingSession": false,
      "providers": [
        {
          "ProviderGuid": "7f006a22-73fb-4c17-b1eb-0a3070f9f187"
        },
        {
          "ProviderGuid": "E6F378E8-21CE-49A9-8D98-1BAAF053AB51"
        },
        {
          "ProviderGuid": "EA289C62-8C36-4904-9726-15ECD282AED5"
        }
      ]
    },
    {
      "type": "JournalCtl",
      "flags": 0,
      "columns": [ "USERID" ]
    },
    {
      "type": "filesystem",
      "interval": "00:00:05"
    },
    {
      "type": "system",
      "interval": "01:00:00"
    },
    {
      "type": "network",
      "interval": "00:00:15"
    },
    {
      "type": "proc",
      "interval": "00:00:5"
    },
    {
      "type": "proc.openfiles",
      "interval": "00:05:00"
    },
    {
      "type": "udp",
      "port": 49999
    },
    {
      "type": "processes",
      "interval": "00:00:30"
    }
  ]
}
